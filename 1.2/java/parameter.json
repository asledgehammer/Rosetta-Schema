{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/asledgehammer/Rosetta-Schema/main/1.2/java/parameter.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string"
    },
    "type": {
      "anyOf": [
        {
          "$ref": "type.json"
        },
        {
          "type": "string"
        }
      ]
    },
    "notes": {
      "type": "string"
    }
  },
  "required": ["name", "type"]
}
