{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/asledgehammer/Rosetta-Schema/main/1.2/java/package.json",
  "type": "object",
  "minItems": 1,
  "additionalProperties": false,
  "properties": {
    "classes": {
      "patternProperties": {
        "^": {
          "$ref": "class.json"
        }
      }
    },
    "packages": {
      "patternProperties": {
        "^": {
          "$ref": "package.json"
        }
      }
    }
  }
}
