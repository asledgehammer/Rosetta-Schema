{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://raw.githubusercontent.com/asledgehammer/PZ-Rosetta-Schema/main/1.2/java/type.json",
    "type": "object",
    "minItems": 1,
    "additionalProperties": false,
    "properties": {
        "full": {
            "description": "The full string version of the type.",
            "type": "string"
        },
        "nullable": {
            "description": "Appends the 'null' type to the list of optional types.",
            "type": "boolean"
        },
        "generics": {
            "$ref": "generics.json"
        }
    }
}