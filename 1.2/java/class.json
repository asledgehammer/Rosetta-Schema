{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/Rosetta-Docs/Rosetta-Docs-Schema/refs/heads/1.2/1.2/java/class.json",
  "type": "object",
  "minItems": 1,
  "additionalProperties": false,
  "properties": {
    "type_parameters": {
      "type": "array",
      "minItems": 1,
      "additionalItems": false,
      "items": {
        "$ref": "type_parameter.json"
      }
    },
    "deprecated": {
      "type": ["boolean", "string"],
      "description": "If the class is deprecated."
    },
    "extends": {
      "type": "object",
      "anyOf": [
        {
          "$ref": "type.json"
        },
        {
          "$ref": "string"
        }
      ]
    },
    "implements": {
      "type": "array",
      "minItems": 1,
      "anyOf": [
        {
          "$ref": "type.json"
        },
        {
          "$ref": "string"
        }
      ]
    },
    "staticFields": {
      "type": "object",
      "minItems": 1,
      "patternProperties": {
        "^": {
          "$ref": "field.json"
        }
      }
    },
    "fields": {
      "type": "object",
      "minItems": 1,
      "patternProperties": {
        "^": {
          "$ref": "field.json"
        }
      }
    },
    "constructors": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "constructor.json"
      }
    },
    "methods": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "method.json"
      }
    },
    "staticMethods": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "method.json"
      }
    },
    "modifiers": {
      "$ref": "modifiers.json"
    },
    "notes": {
      "type": "string"
    },
    "tags": {
      "$ref": "../tags.json"
    }
  }
}
