{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://raw.githubusercontent.com/asledgehammer/PZ-Rosetta-Schema/main/1.1/lua/operator.json",
    "type": "object",
    "minItems": 1,
    "additionalProperties": false,
    "properties": {
        "operation": {
            "type": "string"
        },
        "parameter": {
            "type": "string"
        },
        "return": {
            "type": "string"
        },
        "tags": {
            "$ref": "../tags.json"
        }
    },
    "required": [
        "operation",
        "return"
    ]
}