{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/Rosetta-Docs/Rosetta-Docs-Schema/refs/heads/1.2/1.2/lua/method.json",
  "type": "object",
  "minItems": 1,
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string"
    },
    "static": {
      "type": "boolean",
      "description": "Default: False"
    },
    "deprecated": {
      "type": ["boolean", "string"],
      "description": "If the function is deprecated."
    },
    "parameters": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "parameter.json"
      }
    },
    "return": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "return.json"
      }
    },
    "overloads": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "overload.json"
      }
    },
    "generics": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "generic.json"
      }
    },
    "notes": {
      "type": "string"
    },
    "tags": {
      "$ref": "../tags.json"
    }
  },
  "required": ["name"]
}
