{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://raw.githubusercontent.com/asledgehammer/PZ-Rosetta-Schema/main/1.1/lua/overload.json",
    "type": "object",
    "minItems": 1,
    "additionalProperties": false,
    "properties": {
        "parameters": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "parameter.json"
            }
        },
        "return": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "return.json"
            }
        },
        "notes": {
            "type": "string"
        },
        "tags": {
            "$ref": "../tags.json"
        }
    }
}