{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/asledgehammer/Rosetta-Schema/main/1.2/lua/callback.json",
  "type": "object",
  "minItems": 1,
  "additionalProperties": false,
  "properties": {
    "parameters": {
      "type": "array",
      "minItems": 1,
      "additionalItems": false,
      "items": {
        "$ref": "parameter.json"
      }
    },
    "return": {
      "type": "array",
      "minItems": 1,
      "items": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "$ref": "return.json"
          }
        ]
      }
    },
    "notes": {
      "type": "string"
    },
    "tags": {
      "$ref": "../tags.json"
    }
  }
}
