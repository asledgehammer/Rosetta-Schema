{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/asledgehammer/Rosetta-Schema/main/1.2/lua/constructor.json",
  "type": "object",
  "minItems": 1,
  "additionalProperties": false,
  "properties": {
    "deprecated": {
      "type": ["boolean", "string"],
      "description": "If the function is deprecated."
    },
    "parameters": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "parameter.json"
      }
    },
    "generics": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "generic.json"
      }
    },
    "notes": {
      "type": "string"
    }
  }
}
