{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://raw.githubusercontent.com/asledgehammer/PZ-Rosetta-Schema/main/1.1/lua/constructor.json",
    "type": "object",
    "minItems": 1,
    "additionalProperties": false,
    "properties": {
        "deprecated": {
            "type": "boolean",
            "description": "If the function is deprecated."
        },
        "parameters": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "parameter.json"
            }
        },
        "notes": {
            "type": "string"
        }
    }
}