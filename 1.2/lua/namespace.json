{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://raw.githubusercontent.com/Rosetta-Docs/Rosetta-Docs-Schema/refs/heads/1.2/1.2/lua/namespace.json",
  "type": "object",
  "additionalItems": false,
  "properties": {
    "namespaces": {
      "type": "array",
      "anyOf": [
        {
          "$ref": "namespace.json"
        }
      ]
    },
    "aliases": {
      "type": "object",
      "minItems": 1,
      "patternProperties": {
        "^": {
          "type": "array",
          "minItems": 1,
          "items": {
            "$ref": "alias.json"
          }
        }
      }
    },
    "classes": {
      "type": "object",
      "minItems": 1,
      "additionalProperties": false,
      "patternProperties": {
        "^": {
          "$ref": "class.json"
        }
      }
    },
    "tables": {
      "type": "object",
      "minItems": 1,
      "additionalProperties": false,
      "patternProperties": {
        "^": {
          "$ref": "table.json"
        }
      }
    },
    "functions": {
      "type": "array",
      "minItems": 1,
      "additionalItems": false,
      "items": {
        "$ref": "function.json"
      }
    },
    "fields": {
      "type": "object",
      "minProperties": 1,
      "additionalProperties": false,
      "patternProperties": {
        "^": {
          "$ref": "field.json"
        }
      }
    },
    "notes": {
      "type": "string"
    },
    "tags": {
      "$ref": "../tags.json"
    }
  }
}
