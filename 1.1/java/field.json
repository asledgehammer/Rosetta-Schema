{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://raw.githubusercontent.com/asledgehammer/PZ-Rosetta-Schema/main/1.1/java/field.json",
    "type": "object",
    "minItems": 1,
    "additionalProperties": false,
    "properties": {
        "name": {
            "type": "string"
        },
        "deprecated": {
            "type": "boolean",
            "description": "If the field is deprecated."
        },
        "modifiers": {
            "$ref": "modifiers.json"
        },
        "type": {
            "$ref": "type.json"
        },
        "notes": {
            "type": "string"
        },
        "defaultValue": {
            "type": "string"
        },
        "tags": {
            "$ref": "../tags.json"
        }
    }
}